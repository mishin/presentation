use v5.10;

sub step_by_n {
    my ($start,$end) = @_;
    say "start step_by_n($start,$end)";
    for my $k ( $start .. $end ) {
        say "from step_by_n($start,$end): $k";
	if ($start<=$end){
        step_by_n( $k + 1,$end );
}
        #fork();
    }
}

